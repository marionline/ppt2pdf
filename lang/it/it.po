#: ppt2pdf.sh:51
msgid ""
"\n"
"Usage: ppt2pdf.sh [options[argument]] [filename]\n"
"    options: -r: Recursive,\n"
"    \t     -a: All file in this directory,\n"
"\t     -o: Overwrite existing destination file,\n"
"    \t     -y: Answare YES to all question,\n"
"\t     -v: Verbose (if not use default is 1),\n"
"\t     -d: Include doc file,\n"
"\t     -e: Erase original file,\n"
"\t     -q: Ask before erasing,\n"
"\t     -j: JPEG quality (provides number 0 - 100, default 90),\n"
"\t     -h: Usage.\n"
"    verbose: 0 no output,\n"
"\t     1 a little bit of info,\n"
"\t     2 a lot of info.\n"
"\n"
msgstr ""
"\n"
"Uso: ppt2pdf.sh [opzioni] [nome file]\n"
"    opzioni: -r: Ricorsivo,\n"
"    \t     -a: Tutti i file nella directory,\n"
"\t     -o: Sovrascrivi file di destinazione se esiste,\n"
"    \t     -y: Rispondi YES a tutte le domande,\n"
"\t     -v: Verboso (se non usato di default è 1),\n"
"\t     -d: Includere file doc,\n"
"\t     -e: Eliminare file originale,\n"
"\t     -q: Chiedi prima di cancellare,\n"
"\t     -j: Qualità JPEG (fornire un numero 0 - 100, default 90),\n"
"\t     -h: Uso.\n"
"    verboso: 0 no output,\n"
"\t     1 un po' di informazioni,\n"
"\t     2 un po' più informazioni.\n"
"\n"

